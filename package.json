{
  "name": "pulsar",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "tauri dev",
    "typecheck": "tsc --noEmit",
    "build": "tsc && vite build",
    "lint:ts": "eslint \"**/*.{ts,tsx}\" --fix",
    "prettier": "npx prettier \"**/*.{ts,tsx}\" --write",
    "prepare": "husky install",
    "dev": "vite --force",
    "preview": "vite preview",
    "tauri": "tauri",
    "test:frontend": "vitest run",
    "test:backend": "cd src-tauri && cargo test -- --test-threads=1 --color=auto",
    "test:backend:nocap": "cd src-tauri && cargo test -- --test-threads=1 --color=auto --nocapture",
    "release": "node ./scripts/release.mjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "yarn build-storybook && cd storybook-static && vercel --prod --yes",
    "chromatic": "npx chromatic",
    "clippy": "cd src-tauri && cargo clippy --all-targets --all-features -- -D warnings",
    "nebula:update": "cd src-tauri && cargo update"
  },
  "dependencies": {
    "@huggingface/hub": "^0.15.1",
    "@nozbe/watermelondb": "git+https://github.com/ospfranco/tauri-watermelon.git#1a03ebe1a7ea4052405eb3f80486d82c52e46ed4",
    "@tauri-apps/api": "^1.5.2",
    "effector": "^23.2.0",
    "effector-react": "^23.2.0",
    "nanoid": "^5.0.6",
    "openai": "^4.27.0",
    "preact": "^10.16.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-markdown": "^9.0.1",
    "react-slider": "^2.0.6",
    "react-syntax-highlighter": "^15.5.0",
    "react-tiny-popover": "^8.0.4",
    "react-toastify": "^10.0.5",
    "remark-gfm": "^4.0.0",
    "tauri-plugin-log": "git+https://github.com/tauri-apps/tauri-plugin-log.git",
    "tauri-plugin-sql": "git+https://github.com/tauri-apps/tauri-plugin-sql.git#v1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.23.9",
    "@babel/plugin-transform-runtime": "^7.23.9",
    "@babel/runtime": "^7.24.0",
    "@chromatic-com/storybook": "^1.3.5",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@preact/preset-vite": "^2.5.0",
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/addon-themes": "^8.0.10",
    "@storybook/blocks": "^8.0.10",
    "@storybook/preact": "^8.0.10",
    "@storybook/preact-vite": "^8.0.10",
    "@storybook/test": "^8.0.10",
    "@tauri-apps/cli": "^1.5.8",
    "@types/babel__core": "^7",
    "@types/bun": "^1.0.8",
    "@types/react-custom-scrollbars": "^4",
    "@types/react-slider": "^1.3.6",
    "@types/react-syntax-highlighter": "^15",
    "@types/semver": "^7",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.57.1",
    "aws-sdk": "^2.1584.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "chromatic": "^11.4.0",
    "eslint": "8.39.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-effector": "^0.13.0",
    "eslint-plugin-fsd-arch-validator": "0.0.2",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.3.4",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "8.0.0",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "sass": "^1.70.0",
    "semver": "^7.6.0",
    "storybook": "^8.0.10",
    "typescript": "^5.0.2",
    "vite": "^5.1.5",
    "vite-bundle-visualizer": "^1.1.0",
    "vite-plugin-svgr": "2.4.0",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "packageManager": "yarn@4.1.0"
}

